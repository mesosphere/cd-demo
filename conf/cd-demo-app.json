{
    "container": {
        "docker": {
            "image": "mesosphere/cd-demo-app:latest",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 4000,
                    "protocol": "tcp"
                }
            ]
        },
        "type": "DOCKER"
    },
    "cpus": 0.1,
    "id": "cd-demo-app",
    "instances": 1,
    "labels": {
        "HAPROXY_0_VHOST": "ELB_HOSTNAME",
        "HAPROXY_GROUP": "external",
        "lastChangedBy": "velocity-team@mesosphere.io"
    },
    "mem": 128,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0
    }
}
